# Sepolia Configuration
# Copy this file to .env and fill in your actual values

# Replace with your actual private key (without 0x prefix)
PRIVATE_KEY=your_private_key_here

# Sepolia RPC URL (choose one)
SEPOLIA_RPC_URL=https://sepolia.infura.io/v3/your-infura-key
# Alternative: SEPOLIA_RPC_URL=https://rpc.sepolia.org

# Your wallet address (for reference)
USER_WALLET_ADDRESS=0x14987b6b98A4a2564d0b16c64c1Ed9fc9E974179

# Optional: Etherscan API key for contract verification
ETHERSCAN_API_KEY=your_etherscan_api_key_here
